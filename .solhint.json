{
  "extends": "default",
  "rules": {
    "avoid-throw": false,
    "avoid-suicide": "error",
    "avoid-sha3": "warn",
    "indent": ["warn", 2],
    "compiler-fixed": false,
    "max-line-length": false,
    "not-rely-on-time": false,
    "no-simple-event-func-name": false,
    "func-order": "warn"
  }
}
